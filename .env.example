# Enclypt 2.0 Environment Variables
# Copy this file to .env and customize as needed

# Logging level
RUST_LOG=info

# Server configuration
ENCLYPT2_HOST=127.0.0.1
ENCLYPT2_PORT=8080
ENCLYPT2_API_KEY=your-secret-api-key

# Key storage
ENCLYPT2_KEY_DIR=~/.enclypt2/keys

# Development settings
RUST_BACKTRACE=1
